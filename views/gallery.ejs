<html>
	<%- include('head'); %>
	<body>

		<!-- Wrapper -->
		<div id="wrapper">

			<%- include('header'); %>
			<!-- Main -->
			<div id="main" class="alt">

				<!-- One -->
				<section id="one">
					<div class="inner">
						<header class="major">
							<h1>歷年紀錄</h1>
						</header>
						<% ImageFiles.forEach(function(val, index, arr){ %>
